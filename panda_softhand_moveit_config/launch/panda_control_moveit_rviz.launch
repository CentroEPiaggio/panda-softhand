<?xml version="1.0" ?>
<launch>
  <arg name="robot_ip" default="combined_panda" />
  <arg name="load_gripper" default="true" />
  <arg name="launch_rviz" default="true" />

  <include file="$(find franka_control)/launch/franka_combined_control.launch" >
    <arg name="robot_ip" value="$(arg robot_ip)" />
    <arg name="load_gripper" value="$(arg load_gripper)" />
  </include>

  <include file="$(find panda_softhand_moveit_config)/launch/panda_moveit.launch">
    <arg name="load_gripper" value="$(arg load_gripper)" />
  </include>

  <include file="$(find panda_softhand_moveit_config)/launch/moveit_rviz.launch" if="$(arg launch_rviz)" />
</launch>

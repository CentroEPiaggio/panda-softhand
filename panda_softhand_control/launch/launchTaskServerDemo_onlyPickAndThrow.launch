<?xml version="1.0"?>
<launch>
    
	<!-- Throw Node of Giorgio -->
    
    <include file="$(find franka_throw_unipi)/launch/throw_node.launch" />
	<!-- Launch arguments -->
	<arg name="task_sequences_params_pick_and_throw" default="$(find panda_softhand_control)/configs/task_sequences_params_pick_and_throw.yaml"/>
	
	<!-- Loading config file to param server -->
	<rosparam command="load" file="$(arg task_sequences_params_pick_and_throw)"/>

	<!-- Load main node -->
  	<node name="task_server_demo_only_pickandthrow" pkg="panda_softhand_control" type="task_server_demo_only_pickandthrow" output="screen"/>	

</launch>